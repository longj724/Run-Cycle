<body>
    <div id="nav">
        <h1 id="title">Run Cycle</h2>
    </div>
    
    <div id='map'>
    </div>

    <div id="controller">
        <input id="geocoder" placeholder="Enter Starting Location" required>

        <ul id="potentialLocations"></ul>

        <input id="routeDistance" placeholder="Route Distance" required>

        <button id="makeRoute">Find Route</button>
    </div>

</body>
<script src="/js/apiKeys.js"></script>
<script src="/js/forms.js"></script>
<script src="/js/searchBundle.js"></script>
